{
  "name": "kalshi-solana-pvp",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "dev": "pnpm -C apps/api dev & pnpm -C apps/web dev",
    "build": "pnpm -C apps/api build && pnpm -C apps/web build",
    "docker:up": "docker compose up -d --build",
    "anchor:local": "bash scripts/anchor-local.sh",
    "lint": "pnpm -C apps/web lint",
    "format": "pnpm dlx prettier --write \"**/*.{ts,tsx,js,json,md}\""
  },
  "devDependencies": {
    "@types/node": "^20.11.30"
  },
  "dependencies": {
    "pnpm": "^10.25.0"
  }
}
